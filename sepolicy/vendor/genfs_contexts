# Camera
genfscon proc /qcom_flash                                                  u:object_r:cam_proc:s0
genfscon proc /devinfo/lcd                                                 u:object_r:cam_proc:s0
genfscon proc /rear_eeprom_info                                            u:object_r:cam_proc:s0
genfscon proc /front_lens_id                                               u:object_r:cam_proc:s0
genfscon proc /rear_vcm_info                                               u:object_r:cam_proc:s0
genfscon proc /fuseid                                                      u:object_r:cam_proc:s0
genfscon proc /rear_vcm_id                                                 u:object_r:cam_proc:s0

# Fingerprint
genfscon sysfs /devices/soc/soc:fpc_fpc1020/irq                   u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/soc/soc:fpc_fpc1020/irq_enable            u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/soc/soc:fpc_fpc1020/wakelock_enable       u:object_r:sysfs_fingerprint:s0
genfscon proc /fp_id                                                       u:object_r:fp_proc:s0

# Power
genfscon sysfs /devices/virtual/graphics/fb0                    u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/virtual/graphics/fb1                    u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/virtual/graphics/fb2                    u:object_r:sysfs_graphics:s0
genfscon sysfs /devices/soc/soc:qcom,gpubw                      u:object_r:sysfs_devfreq:s0
genfscon sysfs /devices/soc/soc:qcom,cpubw                      u:object_r:sysfs_devfreq:s0
genfscon sysfs /devices/soc/soc:qcom,mincpubw                   u:object_r:sysfs_devfreq:s0
genfscon sysfs /devices/soc/soc:qcom,memlat-cpu0                u:object_r:sysfs_devfreq:s0
genfscon sysfs /devices/soc/soc:qcom,memlat-cpu4                u:object_r:sysfs_devfreq:s0

# Project version
genfscon proc /oppoVersion                                                 u:object_r:oppo_ver_proc:s0

# Touchpanel
genfscon proc /touchpanel                                       u:object_r:proc_touchpanel:s0

# Zram
genfscon sysfs /devices/virtual/block/zram0/mm_stat             u:object_r:sysfs_zram:s0
